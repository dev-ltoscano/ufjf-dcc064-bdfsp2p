<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="ERROR">
    <Appenders>
        <Console name="consoleAppender" target="SYSTEM_OUT">
            <PatternLayout pattern="[DFS LOG] > Level: %level | Message: %message | Date: %date{HH:mm:ss} | Logger: %logger %n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="ERROR">
            <AppenderRef ref="consoleAppender"/>
        </Root>
    </Loggers>
</Configuration>